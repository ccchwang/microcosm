{
  "name": "microcosm-devtools",
  "version": "0.0.0",
  "description": "Devtools for Microcosm",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --config shells/dev/webpack.config.js",
    "dev:chrome": "webpack --watch --hide-modules --config shells/chrome/webpack.config.js",
    "build": "./bin/build",
    "test": "echo WRITE TESTS; exit 0",
    "zip": "cd shells && zip -r -FS ../dist/chrome.zip chrome -x *src/* -x *webpack.config.js",
    "release": "npm run test && npm run build && npm run zip"
  },
  "keywords": [
    "microcosm",
    "devtools"
  ],
  "license": "MIT",
  "dependencies": {
    "babel-loader": "^7.0.0",
    "circular-json-es6": "^2.0.1",
    "classnames": "^2.2.5",
    "css-loader": "^0.28.4",
    "file-loader": "^0.11.1",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "microcosm": "^12.9.0",
    "paths-js": "^0.4.5",
    "react": "^16.0.0-rc.2",
    "react-dom": "^16.0.0-rc.2",
    "react-hot-loader": "3.0.0-beta.6",
    "react-json-tree": "^0.10.9",
    "style-loader": "^0.18.1",
    "url-loader": "^0.5.6",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.4.5"
  }
}
